{
  "name": "bhuvesh",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:backend": "cd backend && source venv/bin/activate && uvicorn app.main:app --reload --port 8000",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:backend\"",
    "build": "npm run db:generate && next build --turbopack",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit && tsc --noEmit --project tsconfig.jest.json && tsc --noEmit --project tsconfig.cypress.json",
    "type-check:main": "tsc --noEmit",
    "check-all": "npm run type-check && npm run lint && npm run format:check",
    "fix-all": "npm run lint:fix && npm run format",
    "prepare": "npx husky",
    "db:generate": "npx prisma generate",
    "db:push": "npx prisma db push",
    "db:migrate": "npx prisma migrate dev",
    "db:studio": "npx prisma studio",
    "db:seed": "npx prisma db seed",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:component": "cypress run --component",
    "test:component:open": "cypress open --component",
    "test:a11y": "jest --testPathPatterns=a11y",
    "test:all": "npm run test:ci && npm run test:e2e && npm run test:component",
    "test:accessibility": "npm run test:a11y && npm run test:e2e -- --spec 'cypress/e2e/accessibility.cy.js'",
    "test:changed": "jest --onlyChanged",
    "test:staged": "jest --onlyChanged --passWithNoTests",
    "pre-commit:fast": "npm run type-check && npm run lint && npm run test:ci && npm run test:a11y",
    "pre-commit:full": "npm run type-check && npm run lint && npm run format:check && npm run test:ci && npm run test:a11y && npm run test:e2e && npm run build",
    "pre-commit:check": "npm run type-check && npm run lint && npm run format:check && npm run test:ci && npm run test:a11y"
  },
  "dependencies": {
    "@auth/core": "^0.34.2",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hello-pangea/dnd": "^18.0.1",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.18.0",
    "@react-pdf/renderer": "^4.3.1",
    "@reduxjs/toolkit": "^2.9.2",
    "@tiptap/extension-placeholder": "^3.7.2",
    "@tiptap/react": "^3.7.2",
    "@tiptap/starter-kit": "^3.7.2",
    "clsx": "^2.1.1",
    "color-contrast-checker": "^2.1.0",
    "docx": "^9.5.1",
    "file-saver": "^2.0.5",
    "framer-motion": "^12.23.16",
    "gtag": "^1.0.1",
    "html-docx-js": "^0.3.1",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.3",
    "mammoth": "^1.11.0",
    "natural": "^8.1.0",
    "next": "15.5.3",
    "next-auth": "^4.24.11",
    "prisma": "^6.18.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hot-toast": "^2.6.0",
    "react-intersection-observer": "^9.16.0",
    "react-redux": "^9.2.0",
    "react-spring": "^10.0.3",
    "tailwind-merge": "^3.3.1",
    "tailwind-scrollbar": "^4.0.2",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@axe-core/react": "^4.11.0",
    "@eslint/eslintrc": "^3",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/file-saver": "^2.0.7",
    "@types/jest": "^30.0.0",
    "@types/jest-axe": "^3.5.9",
    "@types/node": "^20",
    "@types/pdf-parse": "^1.1.5",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "babel-jest": "^30.2.0",
    "concurrently": "^9.2.1",
    "cypress": "^15.5.0",
    "cypress-axe": "^1.7.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^30.2.0",
    "lint-staged": "^16.1.6",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.17",
    "typescript": "^5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "jest --bail --findRelatedTests"
    ],
    "*.{json,md,css}": [
      "prettier --write"
    ],
    "src/**/*.{ts,tsx}": [
      "npm run test:staged"
    ]
  }
}
