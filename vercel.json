{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "framework": "nextjs",
  "ignoreCommand": "git diff --quiet HEAD^ HEAD ./src ./public ./package.json ./next.config.ts ./tailwind.config.js",
  "git": {
    "deploymentEnabled": {
      "main": true,
      "feature/ats-checker-foundation": true
    }
  }
}
